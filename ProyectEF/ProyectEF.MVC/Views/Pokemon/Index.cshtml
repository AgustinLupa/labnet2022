@model ProyectEF.MVC.Models.Pokemon
@{
    ViewBag.Title = "Pokemones";
    ProyectEF.MVC.Models.PokemonList pokemonList = ViewBag.Result;
}

<br>
<div>
    <p class="bigger">Tabla de Pokemones Api </p>
</div>

<br>
<br>

<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Nombre del pokemon</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var pokemon in pokemonList.PokemonsList)
            {
                <tr>
                    <th scope="row">@pokemon.Name</th>                   
                </tr>
            }
        </tbody>
    </table>
</div>
<br />
<br />

